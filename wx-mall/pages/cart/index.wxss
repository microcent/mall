page{
    height: 100%;
    min-height: 100%;
    background: #f4f4f4;
}
.container{
    background: #f4f4f4;
    width: 100%;
    height: auto;
    min-height: 100%;
    overflow: hidden;
}


.no-login{
    width: 100%;
    height: auto;
    margin: 0 auto;
}

.no-login .c{
    width: 100%;
    height: auto;
    margin-top: 200rpx;
}

.no-login .c image{
    margin: 0 auto;
    display: block;
    text-align: center;
    width: 258rpx;
    height: 258rpx;
}

.no-login .c text{
    margin: 0 auto;
    display: block;
    width: 258rpx;
    height: 59rpx;
    line-height: 29rpx;
    text-align: center;
    font-size: 50rpx;
    color: #999;
}

.no-login button{
    width: 60%;
    margin: 0 auto;
}